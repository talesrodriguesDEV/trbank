{
  "compilerOptions": {
    "target": "ES5",                            
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
